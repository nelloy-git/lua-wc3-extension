{
    "name": "lua-wc3",
    "publisher": "nelloy",
	"displayName": "lua-wc3",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.36.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:lua-wc3.Build"
	],
	"main": "./out/extension.js",
